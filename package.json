{
  "name": "millos-cuisine-explorer",
  "version": "1.0.0",
  "description": "A web application to explore recipes from various African and intercontinental cuisines",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "docker:build": "docker build -t millowise01/millos-cuisine:v1 .",
    "docker:run": "docker run -p 8080:8080 -e API_KEY=secure-api-key-2024 millowise01/millos-cuisine:v1",
    "docker:push": "docker push millowise01/millos-cuisine:v1",
    "compose:up": "docker-compose up -d",
    "compose:down": "docker-compose down"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "axios": "^1.6.0",
    "node-cache": "^5.1.2",
    "helmet": "^7.1.0",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": ["recipes", "cuisine", "food", "api", "african"],
  "author": "Millo",
  "license": "MIT"
}